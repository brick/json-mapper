{
  "name": "brick/json-mapper",
  "description": "Maps JSON data to strongly typed PHP DTOs",
  "type": "library",
  "keywords": [
    "Brick",
    "JSON",
    "Mapper",
    "DTO"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "brick/reflection": "~0.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "vimeo/psalm": "6.14.3"
  },
  "autoload": {
    "psr-4": {
      "Brick\\JsonMapper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Brick\\JsonMapper\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
